<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbsequence" table="TBSEQUENCE" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="year" type="string">
            <column name="year" length="2" not-null="true"/>
        </property>
        <property name="sequence" type="integer">
            <column name="sequence"/>
        </property>
        <property name="product" type="string">
            <column name="product" length="2"/>
        </property>
    </class>
</hibernate-mapping>