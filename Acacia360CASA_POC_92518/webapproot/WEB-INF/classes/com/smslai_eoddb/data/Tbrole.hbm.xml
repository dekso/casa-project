<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbrole" table="TBROLE" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <composite-id name="id" class="com.smslai_eoddb.data.TbroleId">
            <key-property name="roleid" type="string">
                <column name="roleid" length="50"/>
            </key-property>
            <key-property name="rolename" type="string">
                <column name="rolename" length="100"/>
            </key-property>
        </composite-id>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="createddate" type="timestamp">
            <column name="createddate" length="23"/>
        </property>
        <property name="updatedby" type="string">
            <column name="updatedby" length="50"/>
        </property>
        <property name="dateupdated" type="timestamp">
            <column name="dateupdated" length="23"/>
        </property>
    </class>
</hibernate-mapping>