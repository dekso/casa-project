<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbchecksforclearing" table="TBCHECKSFORCLEARING" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="brstn" type="string">
            <column name="brstn" length="20"/>
        </property>
        <property name="checkamount" type="big_decimal">
            <column name="checkamount" precision="16"/>
        </property>
        <property name="checkdate" type="string">
            <column name="checkdate" length="10"/>
        </property>
        <property name="checknumber" type="string">
            <column name="checknumber" length="20"/>
        </property>
        <property name="checktype" type="integer">
            <column name="checktype"/>
        </property>
        <property name="clearingdate" type="string">
            <column name="clearingdate" length="10"/>
        </property>
        <property name="clearingdays" type="integer">
            <column name="clearingdays"/>
        </property>
        <property name="islateclearing" type="boolean">
            <column name="islateclearing"/>
        </property>
        <property name="status" type="string">
            <column name="status" length="2"/>
        </property>
        <property name="accountno" type="string">
            <column name="accountno" length="20"/>
        </property>
        <property name="txrefno" type="string">
            <column name="txrefno" length="20"/>
        </property>
    </class>
</hibernate-mapping>