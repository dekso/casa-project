<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbmerchant" table="TBMERCHANT" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="merchantcode" type="string">
            <column name="merchantcode" length="4"/>
        </property>
        <property name="merchantname" type="string">
            <column name="merchantname" length="150"/>
        </property>
        <property name="accountno" type="string">
            <column name="accountno" length="20"/>
        </property>
        <property name="opendt" type="timestamp">
            <column name="opendt" length="23"/>
        </property>
        <property name="status" type="string">
            <column name="status" length="2"/>
        </property>
        <property name="unit" type="string">
            <column name="unit" length="10"/>
        </property>
        <property name="instcode" type="string">
            <column name="instcode" length="10"/>
        </property>
    </class>
</hibernate-mapping>