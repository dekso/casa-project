<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gldb.data.Tbglentries" table="TBGLENTRIES" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="txdate" type="timestamp">
            <column name="Txdate" length="23" not-null="true"/>
        </property>
        <property name="accountno" type="string">
            <column name="accountno"/>
        </property>
        <property name="txcode" type="string">
            <column name="txcode" length="6"/>
        </property>
        <property name="txctr" type="integer">
            <column name="txctr"/>
        </property>
        <property name="txseqno" type="string">
            <column name="txseqno" length="20"/>
        </property>
        <property name="txcurr" type="string">
            <column name="txcurr" length="3"/>
        </property>
        <property name="txamt" type="double">
            <column name="txamt" precision="53"/>
        </property>
        <property name="glline" type="string">
            <column name="glline" length="7"/>
        </property>
        <property name="gllegveh" type="string">
            <column name="gllegveh" length="5"/>
        </property>
        <property name="glbranch" type="string">
            <column name="glbranch" length="5"/>
        </property>
        <property name="glsl" type="string">
            <column name="glsl" length="15"/>
        </property>
        <property name="debit" type="big_decimal">
            <column name="debit" precision="16"/>
        </property>
        <property name="credit" type="big_decimal">
            <column name="credit" precision="16"/>
        </property>
        <property name="txprod" type="string">
            <column name="txprod" length="5"/>
        </property>
        <property name="txsubprod" type="string">
            <column name="txsubprod" length="5"/>
        </property>
        <property name="txvaldt" type="timestamp">
            <column name="txvaldt" length="23"/>
        </property>
        <property name="txlegveh" type="string">
            <column name="txlegveh" length="5"/>
        </property>
        <property name="txbr" type="string">
            <column name="txbr" length="5"/>
        </property>
        <property name="txglbr" type="string">
            <column name="txglbr" length="5"/>
        </property>
        <property name="costc" type="string">
            <column name="costc" length="10"/>
        </property>
    </class>
</hibernate-mapping>