<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gldb.data.Tbdocsperproduct" table="TBDOCSPERPRODUCT" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="documentcode" type="string">
            <column name="documentcode" length="10"/>
        </property>
        <property name="documentname" type="string">
            <column name="documentname" length="200"/>
        </property>
        <property name="producttype" type="string">
            <column name="producttype" length="5"/>
        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="datecreated" type="timestamp">
            <column name="datecreated" length="23"/>
        </property>
        <property name="updatedby" type="string">
            <column name="updatedby" length="50"/>
        </property>
        <property name="lastupdated" type="timestamp">
            <column name="lastupdated" length="23"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="300"/>
        </property>
    </class>
</hibernate-mapping>