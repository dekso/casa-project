<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gldb.data.Tbdocuments" table="TBDOCUMENTS" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="documentCode" type="string">
            <column name="DocumentCode" length="5"/>
            <generator class="assigned"/>
        </id>
        <property name="documentName" type="string">
            <column name="DocumentName" length="200"/>
        </property>
    </class>
</hibernate-mapping>