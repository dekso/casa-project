<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbsysparam" table="TBSYSPARAM" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="txseq" type="integer">
            <column name="txseq"/>
        </property>
        <property name="seqyy" type="string">
            <column name="seqyy" length="4"/>
        </property>
        <property name="businessdt" type="string">
            <column name="businessdt" length="10"/>
        </property>
        <property name="nextbusinessdt" type="string">
            <column name="nextbusinessdt" length="10"/>
        </property>
        <property name="casaeod" type="integer">
            <column name="casaeod"/>
        </property>
    </class>
</hibernate-mapping>