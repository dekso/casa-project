<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbpassbookissuance" table="TBPASSBOOKISSUANCE" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="accountno" type="string">
            <column name="accountno" length="20"/>
        </property>
        <property name="oldpassbksn" type="string">
            <column name="oldpassbksn" length="20"/>
        </property>
        <property name="newpssbksn" type="string">
            <column name="newpssbksn" length="20"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="250"/>
        </property>
        <property name="issuancetype" type="string">
            <column name="issuancetype" length="10"/>
        </property>
        <property name="txdate" type="timestamp">
            <column name="txdate" length="23"/>
        </property>
        <property name="brid" type="string">
            <column name="brid" length="4"/>
        </property>
        <property name="txby" type="string">
            <column name="txby" length="20"/>
        </property>
    </class>
</hibernate-mapping>