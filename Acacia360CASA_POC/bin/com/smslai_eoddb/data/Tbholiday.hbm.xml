<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbholiday" table="TBHOLIDAY" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="holDate" type="timestamp">
            <column name="hol_date" length="23"/>
        </property>
        <property name="holName" type="string">
            <column name="hol_name" length="50"/>
        </property>
    </class>
</hibernate-mapping>