<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbnetamt" table="TBNETAMT" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="userid" type="string">
            <column name="userid" length="50"/>
        </property>
        <property name="currency" type="string">
            <column name="currency" length="3"/>
        </property>
        <property name="userbalance" type="big_decimal">
            <column name="userbalance" precision="16"/>
        </property>
        <property name="businessdate" type="timestamp">
            <column name="businessdate" length="23"/>
        </property>
    </class>
</hibernate-mapping>