<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbdepositcif" table="TBDEPOSITCIF" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="accountno" type="string">
            <column name="accountno" length="20"/>
        </property>
        <property name="cifno" type="string">
            <column name="cifno" length="20"/>
        </property>
        <property name="cifname" type="string">
            <column name="cifname" length="250"/>
        </property>
    </class>
</hibernate-mapping>