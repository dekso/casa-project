<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbmiscellaneous" table="TBMISCELLANEOUS" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="txdate" type="timestamp">
            <column name="txdate" length="23"/>
        </property>
        <property name="payeename" type="string">
            <column name="payeename" length="100"/>
        </property>
        <property name="payeeacctno" type="string">
            <column name="payeeacctno" length="20"/>
        </property>
        <property name="accountno" type="string">
            <column name="accountno" length="20"/>
        </property>
        <property name="amount" type="big_decimal">
            <column name="amount" precision="16"/>
        </property>
        <property name="currency" type="string">
            <column name="currency" length="3"/>
        </property>
        <property name="reason" type="string">
            <column name="reason" length="10"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="250"/>
        </property>
        <property name="relrecmode" type="string">
            <column name="relrecmode" length="10"/>
        </property>
        <property name="txcode" type="string">
            <column name="txcode" length="6"/>
        </property>
        <property name="txby" type="string">
            <column name="txby" length="20"/>
        </property>
        <property name="checktype" type="string">
            <column name="checktype" length="20"/>
        </property>
        <property name="checkno" type="string">
            <column name="checkno" length="20"/>
        </property>
        <property name="checkdate" type="timestamp">
            <column name="checkdate" length="23"/>
        </property>
        <property name="debitacctno" type="string">
            <column name="debitacctno" length="20"/>
        </property>
        <property name="unitid" type="string">
            <column name="unitid" length="10"/>
        </property>
        <property name="instcode" type="string">
            <column name="instcode" length="10"/>
        </property>
    </class>
</hibernate-mapping>