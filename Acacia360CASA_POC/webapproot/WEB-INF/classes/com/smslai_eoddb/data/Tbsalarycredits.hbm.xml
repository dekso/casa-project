        <property name="tenure2" type="integer">
            <column name="tenure2"/>
        </property>
        <property name="tenure3" type="integer">
            <column name="tenure3"/>
        </property>
        <property name="tenure4" type="integer">
            <column name="tenure4"/>
        </property>
        <property name="tenure5" type="integer">
            <column name="tenure5"/>
        </property>
        <property name="tenureN" type="integer">
            <column name="tenureN"/>
        </property>
    </class>
</hibernate-mapping>