        <property name="eod" type="boolean">
            <column name="eod"/>
        </property>
        <property name="eodPriority" type="integer">
            <column name="eod_priority"/>
        </property>
        <property name="eodInclude" type="boolean">
            <column name="eod_include"/>
        </property>
        <property name="mnemonic" type="string">
            <column name="mnemonic" length="50"/>
        </property>
        <property name="name" type="string">
            <column name="name" length="100"/>
        </property>
        <property name="passbookMnemonic" type="string">
            <column name="passbook_mnemonic" length="10"/>
        </property>
        <property name="passbookMessage" type="string">
            <column name="passbook_message" length="120"/>
        </property>
