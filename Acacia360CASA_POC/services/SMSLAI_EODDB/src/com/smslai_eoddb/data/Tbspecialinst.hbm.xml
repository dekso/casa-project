<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.smslai_eoddb.data.Tbspecialinst" table="TBSPECIALINST" catalog="CASADB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="specialinstcode" type="string">
            <column name="specialinstcode" length="20"/>
        </property>
        <property name="specialinstname" type="string">
            <column name="specialinstname" length="100"/>
        </property>
    </class>
</hibernate-mapping>